{
  "name": "obsidian-audio-recording-timer",
  "version": "0.1.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "node esbuild.config.mjs --watch",
    "prebuild": "npm run lint && npm run typecheck",
    "build:dist": "node esbuild.config.mjs production",
    "build": "npm run build:dist && node scripts/export-build.mjs release",
    "build:debug": "node esbuild.config.mjs && node scripts/export-build.mjs debug",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "verify:release": "node scripts/verify-release.mjs",
    "release:check": "npm run verify:release && npm run build",
    "check": "npm run lint && npm run typecheck"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "0.14.0",
    "@typescript-eslint/parser": "^8.35.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.30.1",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "1.8.7",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.35.1"
  }
}
